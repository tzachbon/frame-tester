{
  "name": "Frame Tester",
  "author": "Tzach Bonfil",
  "version": "1.0.0",
  "description": "This is a Frame tester extension, test your Web App by mocking popular frames",
  "icons": {
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "cookies",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": ["js/popup.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Frame Tester",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "images": [],
  "web_accessible_resources": [
    "assets/images/popup-background.png",
    "assets/images/white_dots01.svg",
    "assets/images/white_dots02-01.svg",
    "assets/images/white_dots03.svg",
    "assets/images/white_dots04.svg"
  ],
  "content_scripts": []
}
